{
  "name": "ag-charts-website",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ag-charts-website/src",
  "targets": {
    "build": {
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "executor": "@nxtensions/astro:build",
      "options": {},
      "configurations": {
        "staging": {}
      }
    },
    "dev": {
      "executor": "@nxtensions/astro:dev",
      "options": {
        "port": 4600
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "ag-charts-community"
        },
        {
          "target": "build",
          "projects": "ag-charts-enterprise"
        }
      ]
    },
    "preview": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "executor": "@nxtensions/astro:preview",
      "options": {},
      "configurations": {
        "staging": {}
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ag-charts-website/**/*.ts"],
        "eslintConfig": "packages/ag-charts-website/.eslintrc.json"
      }
    },
    "check": {
      "executor": "@nxtensions/astro:check"
    },
    "sync": {
      "executor": "@nxtensions/astro:sync"
    }
  },
  "tags": []
}
