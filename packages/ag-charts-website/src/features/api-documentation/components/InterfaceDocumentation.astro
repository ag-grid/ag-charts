---
import { InterfaceDocumentation } from './InterfaceDocumentation';
import { getJsonFromDevFile } from '@utils/getJsonFromDevFile';

const interfaceLookup = getJsonFromDevFile('ag-charts-community/interfaces.AUTO.json');
const codeLookup = getJsonFromDevFile('ag-charts-community/doc-interfaces.AUTO.json');

const { config: configString, names: namesString, exclude: excludeString, ...props } = Astro.props;
const config = configString ? JSON.parse(configString) : {};
const names = namesString ? JSON.parse(namesString) : undefined;
const exclude = excludeString ? JSON.parse(excludeString) : undefined;
---

<InterfaceDocumentation
    client:load
    interfaceLookup={interfaceLookup}
    codeLookup={codeLookup}
    config={config}
    names={names}
    exclude={exclude}
    {...props}
/>
