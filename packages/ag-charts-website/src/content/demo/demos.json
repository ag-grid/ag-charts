                {
                    "name": "Simple Bar",
                    "description": "Bar charts represent data using horizontal bars, with the length of each bar proportional to the value being plotted. They are useful for categorical data, and are simple to interpret. They work best where the number of data points is limited.",
                    "example": "simple-bar"
                },
                {
                    "name": "Grouped Bar",
                    "description": "Grouped bar charts (also known as clustered bar charts) show multiple series with the <a href='./simple-bar/'>bars</a> grouped by category, to allow for easier comparisons across different series.",
                    "example": "grouped-bar"
                },
                {
                    "name": "Stacked Bar",
                    "description": "Stacked bar charts allow part-to-whole comparisons, with series stacked from left to right in the horizontal <a href='./simple-bar/'>bars</a>. They make it easy to compare combined bar lengths.",
                    "example": "stacked-bar"
                },
                {
                    "name": "100% Stacked Bar",
                    "description": "100% stacked bar charts show the relative percentage of multiple series in <a href='./stacked-bar/'>stacked bars</a>, where each stacked bar always totals to 100%.",
                    "example": "100--stacked-bar"
                },
                {
                    "name": "Bar With Labels",
                    "description": "Using labels in a <a href='./simple-bar/'>bar chart</a> can be useful to highlight the values of each bar.",
                    "example": "bar-with-labels"
                }
            ]
        },
        {
            "name": "Column",
