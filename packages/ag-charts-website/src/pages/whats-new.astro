---
import { getEntry } from 'astro:content';
import WhatsNew from '@ag-website-shared/components/whats-new/pages/whats-new.astro';

const { data: versionsData } = await getEntry('versions', 'ag-charts-versions');
const { data: docsNavData } = await getEntry('docs-nav', 'docsNav');
---

<WhatsNew site="charts" versionsData={versionsData} menuData={docsNavData} />
