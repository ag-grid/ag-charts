{
  "name": "ag-charts-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ag-charts-angular/projects",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "inputs": ["tsDefaults", "{projectRoot}/angular.json", "{projectRoot}/projects/**"],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "script": "build"
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["{workspaceRoot}/dist/packages/ag-charts-angular.tgz"],
      "options": {
        "cwd": "packages/ag-charts-angular/dist",
        "commands": [
          {
            "command": "mkdir -p ../../../dist/packages/"
          },
          {
            "command": "yarn pack -f ../../../dist/packages/ag-charts-angular.tgz"
          }
        ]
      }
    }
  },
  "tags": []
}
