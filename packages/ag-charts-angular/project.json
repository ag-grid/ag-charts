{
  "name": "ag-charts-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ag-charts-angular/projects",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "inputs": ["tsDefaults", "{projectRoot}/angular.json", "{projectRoot}/projects/**"],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "script": "build"
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": [
        "{workspaceRoot}/dist/packages/ag-charts-angular.tgz",
        "{workspaceRoot}/dist/packages/contents/ag-charts-angular"
      ],
      "options": {
        "cwd": "packages/ag-charts-angular",
        "commands": [
          {
            "command": "mkdir -p ../../dist/packages/contents/ag-charts-angular"
          },
          {
            "command": "yarn pack -f ../../dist/packages/ag-charts-angular.tgz"
          },
          {
            "command": "tar -xf ../../dist/packages/ag-charts-angular.tgz -C ../../dist/packages/contents/ag-charts-angular"
          }
        ]
      }
    }
  },
  "tags": []
}
