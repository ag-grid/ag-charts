{
  "name": "ag-charts-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-charts-react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "nx:noop"
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "inputs": ["tsDefaults", "{projectRoot}/package.json"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/ag-charts-react/dist",
        "generateExportsField": false,
        "tsConfig": "packages/ag-charts-react/tsconfig.lib.json",
        "project": "packages/ag-charts-react/package.json",
        "entryFile": "packages/ag-charts-react/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/ag-charts-react/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "format": ["cjs", "esm"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/ag-charts-react/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["dist/packages/ag-charts-react.tgz"],
      "options": {
        "cwd": "packages/ag-charts-react/dist",
        "commands": [
          { "command": "mkdir -p ../../../dist/packages/" },
          { "command": "yarn pack -f ../../../dist/packages/ag-charts-react.tgz" }
        ]
      }
    }
  }
}
