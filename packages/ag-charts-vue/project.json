{
  "name": "ag-charts-vue",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ag-charts-vue/src",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "inputs": [
        "tsDefaults",
        "{projectRoot}/(vue|postcss).config.js",
        "{projectRoot}/src/**",
        "{projectRoot}/main.(js|d.ts)"
      ],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "script": "build"
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["dist/packages/ag-charts-vue.tgz"],
      "options": {
        "cwd": "packages/ag-charts-vue",
        "commands": [
          { "command": "mkdir -p ../../dist/packages/" },
          { "command": "yarn pack -f ../../dist/packages/ag-charts-vue.tgz" }
        ]
      }
    }
  },
  "tags": []
}
