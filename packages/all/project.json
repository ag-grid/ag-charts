{
  "name": "all",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/all/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "dependsOn": ["^build"]
    },
    "build-tsc": {
      "executor": "nx:noop",
      "dependsOn": ["^build-tsc"]
    },
    "lint": {
      "executor": "nx:noop",
      "dependsOn": ["^lint"],
      "configurations": {
        "fix": {}
      }
    },
    "test": {
      "executor": "nx:noop",
      "dependsOn": ["^test"],
      "configurations": {
        "update": {}
      }
    },
    "pack": {
      "executor": "nx:noop",
      "dependsOn": ["^pack"],
      "configurations": {
        "production": {}
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": true,
        "commands": [
          {
            "command": "while true ; do nx watch -d -p ag-charts-community,ag-charts-enterprise -- nx run ag-charts-enterprise:build --configuration watch ; done"
          },
          {
            "command": "while true ; do nx watch -d -p ag-charts-community -- nx run ag-charts-community:docs-resolved-interfaces ; done"
          },
          {
            "command": "nx run ag-charts-website:dev --configuration watch"
          }
        ]
      },
      "configurations": {
        "windows": {
          "commands": [{ "command": "nx run ag-charts-website:dev --configuration watch" }]
        }
      }
    },
    "format-check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx format:check --all"
          }
        ]
      }
    },
    "blt": {
      "executor": "nx:noop",
      "dependsOn": ["build", "lint", "format-check", "test"],
      "configurations": {
        "update": {},
        "production": {}
      }
    }
  },
  "implicitDependencies": [
    "ag-charts-community",
    "ag-charts-enterprise",
    "ag-charts-community-examples",
    "ag-charts-website",
    "ag-charts-website-e2e",
    "ag-charts-angular",
    "ag-charts-react",
    "ag-charts-vue",
    "ag-charts-vue3",
    "ag-charts-build-tools"
  ],
  "tags": []
}
