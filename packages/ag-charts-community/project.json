{
  "name": "ag-charts-community",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-charts-community/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "dependsOn": ["build:types", "build:package", "build:umd"],
      "inputs": [],
      "outputs": [],
      "configurations": {
        "watch": {},
        "production": {}
      }
    },
    "build:types": {
      "executor": "@nx/js:tsc",
      "dependsOn": ["^build:types"],
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/src/**/*",
        "{projectRoot}/tsconfig.*.json",
        "tsDeclarations",
        "sharedGlobals",
        "defaultExcludes",
        {
          "externalDependencies": ["npm:typescript"]
        }
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/ag-charts-community/dist/types",
        "main": "packages/ag-charts-community/src/main.ts",
        "tsConfig": "packages/ag-charts-community/tsconfig.types.json",
        "compiler": "tsc"
      },
      "configurations": {
        "watch": {
          "tsConfig": "packages/ag-charts-community/tsconfig.types.watch.json"
        },
        "production": {}
      }
    },
    "build:test": {
      "dependsOn": ["^build:types", "build:types"],
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/src/**/*",
        "{projectRoot}/tsconfig.*.json",
        "tsDeclarations",
        "sharedGlobals",
        "buildOutputExcludes",
        {
          "externalDependencies": ["npm:typescript"]
        }
      ],
      "outputs": ["{projectRoot}/dist/test"],
      "command": "npx tsc -p {projectRoot}/tsconfig.spec.json --outDir {projectRoot}/dist/test",
      "configurations": {
        "watch": {},
        "production": {}
      }
    },
    "build:package": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": ["^build"],
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/src/**/*",
        "{projectRoot}/tsconfig.*.json",
        "tsDeclarations",
        "sharedGlobals",
        "defaultExcludes",
        {
          "externalDependencies": ["npm:typescript", "npm:esbuild"]
        }
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist/package",
        "main": "{projectRoot}/src/main.ts",
        "tsConfig": "{projectRoot}/tsconfig.lib.json",
        "esbuildConfig": "esbuild.config.cjs",
        "platform": "browser",
        "target": "es2020",
        "format": ["cjs", "esm"],
        "sourcemap": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "{projectRoot}/tsconfig.watch.json"
        },
        "staging": {
          "sourcemap": false
        },
        "production": {
          "sourcemap": false
        }
      }
    },
    "build:umd": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": ["build:package"],
      "inputs": ["jsOutputs"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist/umd",
        "outputFileName": "ag-charts-community",
        "main": "{projectRoot}/dist/package/main.cjs.js",
        "tsConfig": "{projectRoot}/tsconfig.lib.json",
        "esbuildConfig": "esbuild.config.cjs",
        "platform": "browser",
        "target": "es2020",
        "bundle": true,
        "thirdParty": true,
        "skipTypeCheck": true,
        "external": [],
        "format": ["cjs"],
        "sourcemap": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "{projectRoot}/tsconfig.watch.json"
        },
        "staging": {
          "sourcemap": false
        },
        "production": {
          "sourcemap": false
        }
      }
    },
    "docs-debug-interfaces": {
      "executor": "ag-charts-generate-code-reference-files:generate",
      "dependsOn": ["ag-charts-generate-code-reference-files:build"],
      "inputs": ["{projectRoot}/src/options/**/*.ts", { "dependentTasksOutputFiles": "**/*", "transitive": false }],
      "outputs": ["{options.output}"],
      "cache": true,
      "options": {
        "mode": "debug-interfaces",
        "inputs": ["packages/ag-charts-community/src/options"],
        "output": "dist/packages/ag-charts-community/debug-interfaces.AUTO.json"
      }
    },
    "docs-resolved-interfaces": {
      "executor": "ag-charts-generate-code-reference-files:generate",
      "dependsOn": ["ag-charts-generate-code-reference-files:build"],
      "inputs": ["{projectRoot}/src/options/**/*.ts", { "dependentTasksOutputFiles": "**/*", "transitive": false }],
      "outputs": ["{options.output}"],
      "cache": true,
      "options": {
        "mode": "docs-interfaces",
        "inputs": ["packages/ag-charts-community/src/options"],
        "output": "dist/packages/ag-charts-community/resolved-interfaces.AUTO.json"
      }
    },
    "lint:eslint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "eslintConfig": "{projectRoot}/.eslintrc.js"
      },
      "configurations": {
        "fix": {
          "fix": true
        }
      }
    },
    "lint:circular": {
      "executor": "nx:run-script",
      "inputs": ["{projectRoot}/src/**"],
      "outputs": [],
      "options": {
        "script": "lint:circular"
      }
    },
    "lint:depcruise": {
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/src/**", "{projectRoot}/.dependency-cruiser.js"],
      "outputs": [],
      "options": {
        "cwd": "packages/ag-charts-community",
        "commands": ["npx depcruise src"]
      }
    },
    "lint": {
      "executor": "nx:noop",
      "dependsOn": ["lint:eslint", "lint:circular", "lint:depcruise"],
      "configurations": {
        "fix": {}
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}", "{workspaceRoot}/reports/ag-charts-community.xml"],
      "options": {
        "jestConfig": "packages/ag-charts-community/jest.config.ts",
        "testPathIgnorePatterns": [".*/benchmarks/.*"],
        "passWithNoTests": false
      },
      "configurations": {
        "watch": {
          "watch": true
        },
        "update": {
          "updateSnapshot": true
        }
      },
      "dependsOn": [
        "build:test",
        {
          "projects": ["ag-charts-community-examples"],
          "target": "build"
        }
      ]
    },
    "benchmark": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["jsOutputs"],
      "outputs": ["{workspaceRoot}/reports/{projectRoot}/benchmarks"],
      "options": {
        "commands": [
          "find {projectRoot}/benchmarks -name '*.test.ts' | xargs -L1 -P1 node --expose-gc ./node_modules/jest/bin/jest.js --config {projectRoot}/jest.config.ts --runInBand"
        ]
      },
      "configurations": {
        "update": {
          "commands": [
            "find {projectRoot}/benchmarks -name '*.test.ts' | xargs -L1 -P1 node node --expose-gc ./node_modules/jest/bin/jest.js --config {projectRoot}/jest.config.ts --runInBand -u"
          ]
        }
      },
      "dependsOn": [
        "build",
        "ag-charts-website-benchmarks_integrated-large-scale_main.ts:generate-example",
        "ag-charts-website-benchmarks_large-dataset_main.ts:generate-example",
        "ag-charts-website-benchmarks_large-scale-multi-series_main.ts:generate-example",
        "ag-charts-website-benchmarks_multi-series_main.ts:generate-example",
        "ag-charts-website-benchmarks_resize_main.ts:generate-example"
      ]
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "inputs": ["allTransitiveOutputs"],
      "outputs": ["{workspaceRoot}/dist/{projectRoot}.tgz"],
      "options": {
        "cwd": "packages/ag-charts-community",
        "parallel": false,
        "commands": ["mkdir -p ../../dist/packages", "yarn pack -f ../../dist/packages/ag-charts-community.tgz"]
      }
    },
    "pack:extract": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack"],
      "inputs": ["allOutputs"],
      "outputs": ["{workspaceRoot}/dist/packages/contents/ag-charts-community/"],
      "options": { "command": "tools/package/extract-contents.sh {projectRoot}" }
    },
    "pack:verify": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack:extract"],
      "inputs": ["allOutputs"],
      "options": {
        "commands": ["node tools/package/sanity-check-package.js dist/packages/contents/ag-charts-community/package"]
      }
    }
  },
  "implicitDependencies": ["ag-charts-test", "ag-charts-locale"],
  "tags": ["no-batching"]
}
