{
  "name": "ag-charts-community",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-charts-community/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "inputs": ["tsDefaults"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/ag-charts-community/dist",
        "main": "packages/ag-charts-community/src/main.ts",
        "outputFileName": "ag-charts-community",
        "tsConfig": "packages/ag-charts-community/tsconfig.lib.json",
        "rollupConfig": "packages/ag-charts-community/rollup.config.cjs",
        "assets": [
          { "input": "./", "glob": "LICENSE.*", "output": "./" },
          { "input": "../../", "glob": "README.*", "output": "./" }
        ],
        "project": "packages/ag-charts-community/package.json",
        "compiler": "tsc",
        "format": ["cjs", "esm"],
        "generateExportsField": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "packages/ag-charts-community/tsconfig.watch.json"
        },
        "production": {
          "format": ["cjs", "esm"]
        }
      }
    },
    "interface-metadata": {
      "executor": "ag-charts-build-tools:generate-code-reference-files",
      "inputs": ["{projectRoot}/src"],
      "outputPath": "{options.output}",
      "options": {
        "mode": "interfaces",
        "inputs": ["packages/ag-charts-community/src"],
        "output": "dist/packages/ag-charts-community/interfaces.AUTO.json"
      }
    },
    "interface-docs": {
      "executor": "ag-charts-build-tools:generate-code-reference-files",
      "inputs": ["{projectRoot}/src"],
      "outputPath": "{options.output}",
      "options": {
        "mode": "docs",
        "inputs": ["packages/ag-charts-community/src"],
        "output": "dist/packages/ag-charts-community/doc-interfaces.AUTO.json"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ag-charts-community/**/*.ts", "packages/ag-charts-community/package.json"],
        "eslintConfig": "packages/ag-charts-community/.eslintrc.json"
      },
      "configurations": {
        "fix": {
          "fix": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}", "{workspaceRoot}/reports/ag-charts-community.xml"],
      "options": {
        "jestConfig": "packages/ag-charts-community/jest.config.ts"
      },
      "configurations": {
        "ci": {
          "ci": true
        },
        "watch": {
          "watch": true
        },
        "update": {
          "updateSnapshot": true
        }
      },
      "dependsOn": [
        {
          "projects": ["ag-charts-community-examples"],
          "target": "build"
        }
      ]
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["{workspaceRoot}/dist/packages/ag-charts-community.tgz"],
      "options": {
        "cwd": "packages/ag-charts-community/dist",
        "commands": [
          {
            "command": "mkdir -p ../../../dist/packages/"
          },
          {
            "command": "yarn pack -f ../../../dist/packages/ag-charts-community.tgz"
          }
        ]
      }
    }
  },
  "tags": []
}
