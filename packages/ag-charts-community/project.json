{
  "name": "ag-charts-community",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-charts-community/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ag-charts-community",
        "main": "packages/ag-charts-community/src/main.ts",
        "tsConfig": "packages/ag-charts-community/tsconfig.lib.json",
        "assets": [],
        "project": "packages/ag-charts-community/package.json",
        "compiler": "tsc"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ag-charts-community/**/*.ts"],
        "eslintConfig": "packages/ag-charts-community/.eslintrc.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/ag-charts-community/jest.config.ts"
      },
      "configurations": {
        "ci": {
          "ci": true
        },
        "watch": {
          "watch": true
        },
        "update": {
          "updateSnapshot": true
        }
      },
      "dependsOn": [{ "projects": ["ag-charts-community-examples"], "target": "build" }]
    }
  },
  "tags": []
}
