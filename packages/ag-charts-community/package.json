{
  "name": "ag-charts-community",
  "version": "7.3.0",
  "description": "Advanced Charting / Charts supporting Javascript / Typescript / React / Angular / Vue",
  "main": "./dist/cjs/es5/main.js",
  "module": "./dist/esm/es5/main.js",
  "types": "./dist/cjs/es5/main.d.ts",
  "scripts": {
    "clean": "rimraf dist typings .hash",
    "test:ci": "export FONTCONFIG_PATH=$PWD/src/chart/test FONTCONFIG_FILE=$PWD/src/chart/test/fonts.conf PANGOCAIRO_BACKEND=fontconfig ; npx jest --runInBand",
    "test:dev": "npm run test:lint ; export FC_DEBUG=0 FONTCONFIG_PATH=$PWD/src/chart/test FONTCONFIG_FILE=$PWD/src/chart/test/fonts.conf PANGOCAIRO_BACKEND=fontconfig ; npx jest",
    "test:lint": "npm run test-circular-deps",
    "test": "if test -z \"$TEAMCITY_PROJECT_NAME\" ; then exec npm run test:dev ; else exec npm run test:ci ; fi",
    "test-watch": "npm run test:dev -- --watch",
    "test-coverage": "npm run test:dev -- --coverage",
    "test-debug": "export FONTCONFIG_PATH=$PWD/src/chart/test FONTCONFIG_FILE=$PWD/src/chart/test/fonts.conf PANGOCAIRO_BACKEND=fontconfig ; node --inspect-brk=0.0.0.0 node_modules/.bin/jest --watch --no-cache",
    "test-update": "export SNAPSHOT_FAILURE_THRESHOLD=0.001 ; npm run test:dev -- -u",
    "test-circular-deps": "npx madge --circular --extensions ts src/",
    "build-cjs": "rimraf dist/tsconfig.cjs.es5.docs.tsbuildinfo && npx tsc -p tsconfig.cjs.es5.json && npx tsc -p tsconfig.cjs.es6.json",
    "build-esm": "npx tsc -p tsconfig.esm.es5.json && npx tsc -p tsconfig.esm.es6.json",
    "build-cjs-prod": "npx tsc -p tsconfig.cjs.es5.json --sourceMap false && npx tsc -p tsconfig.cjs.es6.json --sourceMap false",
    "build-esm-prod": "npx tsc -p tsconfig.esm.es5.json --sourceMap false && npx tsc -p tsconfig.esm.es6.json --sourceMap false",
    "package": "node ../../module-build/rollup/build.js --bundlePrefix ag-charts-community --umdModuleName agCharts",
    "build": "npm run build-cjs && npm run build-esm && npx tsc -p tsconfig.typings.json && npm run hash",
    "build-prod": "npm run build-cjs-prod && npm run build-esm-prod && tsc -p tsconfig.typings.json && npm run hash",
    "watch": "npx tsc -p tsconfig.json --watch",
    "watch-esm": "tsc -p tsconfig.esm.es5.json -w",
    "hash": "sh ../../scripts/hashDirectory.sh > .hash",
    "install-hooks": "cd ../.. && husky install charts-community-modules/ag-charts-community/.husky",
    "fix": "run-s fix:*",
    "fix:prettier": "npx prettier -w .",
    "fix:eslint": "npx eslint --fix src/ --ext .js,.ts",
    "fix:generated-docs": "npx lerna run --scope @ag-grid-community/all-modules generate-doc-files",
    "lint": "run-s lint:*",
    "lint:prettier": "npx prettier -c .",
    "lint:eslint": "npx eslint src/ --ext .js,.ts",
    "predocker:init": "docker build -t charts:latest .docker",
    "docker:init": ".docker/run.sh init",
    "docker:run": ".docker/run.sh run",
    "docker:test-watch": ".docker/run.sh run npm run test-watch",
    "docker:test-coverage": ".docker/run.sh run npm run test-coverage",
    "docker:test-debug": ".docker/run.sh run npm run test-debug",
    "docker:test-update": ".docker/run.sh run npm run test-update",
    "docker:clean": ".docker/run.sh clean",
    "dev-server": "node ./tools/dev-server/index.js --port=2020 --debounce=250 --es=2015 --sourcemap=true",
    "generate-chart-gallery": "npx ts-node -T tools/gallery-thumbnail-generator.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "keywords": [
    "chart",
    "charts",
    "data",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "reactjs",
    "vue",
    "vuejs"
  ],
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie >= 0",
    "not ie_mob >= 0",
    "not blackberry > 0",
    "not op_mini all",
    "not operamobile >= 0"
  ],
  "homepage": "https://www.ag-grid.com/",
  "devDependencies": {
    "@babel/types": "7.12.7",
    "@types/jest": "^27.4.1",
    "@types/jest-image-snapshot": "^5.1.0",
    "@types/node": "^16.0.0",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "canvas": "^2.10.2",
    "eslint": "^8.35.0",
    "glob": "8.0.3",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-image-snapshot": "^6.0.0",
    "jest-runner": "^27.5.1",
    "jest-serial-runner": "^1.2.1",
    "lint-staged": "^13.0.3",
    "madge": "6.0.0",
    "npm-run-all": "^4.1.5",
    "pixelmatch": "^5.2.1",
    "prettier": "^2.8.3",
    "raf": "3.4.1",
    "rimraf": "3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "~4.0.8",
    "ws": "8.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,js,md,json}": "charts-community-modules/ag-charts-community/.lint-staged/run.sh prettier",
    "*.{ts,js}": "charts-community-modules/ag-charts-community/.lint-staged/run.sh eslint",
    "*.ts": "charts-community-modules/ag-charts-community/.lint-staged/run.sh autodocs"
  }
}