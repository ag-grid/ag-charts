{
  "name": "ag-charts-locale",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-charts-locale/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "dependsOn": ["build:types", "build:package", "build:umd"],
      "inputs": [],
      "outputs": [],
      "configurations": {
        "watch": {},
        "production": {}
      }
    },
    "build:types": {
      "executor": "@nx/js:tsc",
      "dependsOn": ["^build:types"],
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/src/**/*",
        "{projectRoot}/tsconfig.*.json",
        "tsDeclarations",
        "sharedGlobals",
        "defaultExcludes",
        {
          "externalDependencies": ["npm:typescript"]
        }
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist/types",
        "main": "{projectRoot}/src/main.ts",
        "tsConfig": "{projectRoot}/tsconfig.types.json",
        "compiler": "tsc"
      },
      "configurations": {
        "watch": {
          "tsConfig": "packages/ag-charts-locale/tsconfig.types.watch.json"
        },
        "production": {}
      }
    },
    "build:test": {
      "dependsOn": ["^build:types", "build:types"],
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/src/**/*",
        "{projectRoot}/tsconfig.*.json",
        "tsDeclarations",
        "sharedGlobals",
        "buildOutputExcludes",
        {
          "externalDependencies": ["npm:typescript"]
        }
      ],
      "command": "npx tsc -p {projectRoot}/tsconfig.spec.json",
      "configurations": {
        "watch": {},
        "production": {}
      }
    },
    "build:package": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": ["^build"],
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/src/**/*",
        "{projectRoot}/tsconfig.*.json",
        "tsDeclarations",
        "sharedGlobals",
        "defaultExcludes",
        {
          "externalDependencies": ["npm:typescript", "npm:esbuild"]
        }
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist/package",
        "main": "{projectRoot}/src/*.ts",
        "tsConfig": "{projectRoot}/tsconfig.lib.json",
        "esbuildConfig": "esbuild.config.cjs",
        "platform": "browser",
        "target": "es6",
        "format": ["cjs", "esm"],
        "sourcemap": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "{projectRoot}/tsconfig.watch.json"
        },
        "production": {
          "sourcemap": false
        }
      }
    },
    "build:umd": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": ["build:package"],
      "inputs": ["jsOutputs"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist/umd",
        "outputFileName": "ag-charts-locale",
        "main": "{projectRoot}/dist/package/main.cjs.js",
        "tsConfig": "{projectRoot}/tsconfig.lib.json",
        "esbuildConfig": "esbuild.config.cjs",
        "platform": "browser",
        "target": "es6",
        "bundle": true,
        "thirdParty": true,
        "skipTypeCheck": true,
        "external": [],
        "format": ["cjs"],
        "sourcemap": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "{projectRoot}/tsconfig.watch.json"
        },
        "production": {
          "sourcemap": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "eslintConfig": "{projectRoot}/.eslintrc.js"
      },
      "configurations": {
        "fix": {
          "fix": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}", "{workspaceRoot}/reports/ag-charts-locale.xml"],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.ts",
        "passWithNoTests": false
      },
      "configurations": {
        "watch": {
          "watch": true
        },
        "update": {
          "updateSnapshot": true
        }
      },
      "dependsOn": ["build:test"]
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["{workspaceRoot}/dist/{projectRoot}.tgz"],
      "options": {
        "cwd": "packages/ag-charts-locale",
        "parallel": false,
        "commands": ["mkdir -p ../../dist/packages", "yarn pack -f ../../dist/packages/ag-charts-locale.tgz"]
      }
    },
    "pack:extract": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack"],
      "inputs": ["{workspaceRoot}/dist/{projectRoot}.tgz"],
      "outputs": ["{workspaceRoot}/dist/packages/contents/ag-charts-locale/"],
      "options": {
        "parallel": false,
        "commands": [
          "mkdir -p dist/packages/contents/ag-charts-locale",
          "tar -xzf dist/packages/ag-charts-locale.tgz -C dist/packages/contents/ag-charts-locale/"
        ]
      }
    },
    "pack:verify": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack:extract"],
      "inputs": ["{workspaceRoot}/dist/packages/contents/ag-charts-locale/"],
      "options": {
        "commands": ["node tools/sanity-check-package.js dist/packages/contents/ag-charts-locale/package"]
      }
    }
  },
  "implicitDependencies": [],
  "tags": ["no-batching", "no-sharding"]
}
