{
  "name": "ag-charts-vue",
  "description": "AG Charts Vue Component",
  "version": "5.3.0",
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "MIT",
  "main": "main.js",
  "typings": "main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "homepage": "https://www.ag-grid.com/",
  "keywords": [
    "chart",
    "data",
    "charts",
    "graph",
    "vue",
    "vuejs"
  ],
  "scripts": {
    "clean": "rimraf dist lib .hash",
    "bundle": "npx vue-cli-service build --target lib src/AgChartsVue.ts",
    "npm": "npx tsc -p tsconfig-lib.json",
    "build": "npm run npm && npm run bundle && npm run hash",
    "build-prod": "npm run build",
    "lint": "npx vue-cli-service lint",
    "hash": "sh ../../scripts/hashDirectory.sh > .hash"
  },
  "devDependencies": {
    "ag-charts-community": "~5.3.0",
    "@types/node": "12.20.20",
    "@vue/cli-plugin-typescript": "3.12.1",
    "@vue/cli-plugin-unit-jest": "3.12.1",
    "@vue/cli-service": "3.12.1",
    "typescript": "3.9.10",
    "vue": "2.6.10",
    "vue-class-component": "7.2.6",
    "vue-property-decorator": "8.5.1",
    "vue-template-compiler": "2.6.14",
    "@babel/traverse":"7.18.5",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "ag-charts-community": "~5.3.0",
    "vue": ">= 2.2 <= 2.5.17 || >= 2.5.20",
    "vue-property-decorator": "^7.2.0 || ^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
