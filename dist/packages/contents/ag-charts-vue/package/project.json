{
  "name": "ag-charts-vue",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ag-charts-vue/src",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "inputs": [
        "tsDefaults",
        "{projectRoot}/(vue|postcss).config.js",
        "{projectRoot}/src/**",
        "{projectRoot}/main.(js|d.ts)"
      ],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "script": "build"
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["{workspaceRoot}/dist/packages/ag-charts-vue.tgz"],
      "options": {
        "cwd": "packages/ag-charts-vue",
        "commands": [
          {
            "command": "mkdir -p ../../dist/packages"
          },
          {
            "command": "yarn pack -f ../../dist/packages/ag-charts-vue.tgz"
          }
        ]
      }
    },
    "pack:extract": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack"],
      "outputs": ["{workspaceRoot}/dist/packages/contents/ag-charts-vue/"],
      "options": {
        "commands": [
          {
            "command": "mkdir -p dist/packages/contents/ag-charts-vue"
          },
          {
            "command": "tar -xzf dist/packages/ag-charts-vue.tgz -C dist/packages/contents/ag-charts-vue/"
          }
        ]
      }
    },
    "pack:verify": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack:extract"],
      "options": {
        "commands": [
          {
            "command": "node tools/sanity-check-package.js dist/packages/contents/ag-charts-vue/package"
          }
        ]
      }
    }
  },
  "tags": []
}
