{
  "name": "ag-charts-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ag-charts-angular/projects",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "inputs": ["tsDefaults", "{projectRoot}/angular.json", "{projectRoot}/projects/**"],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "script": "build"
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["{workspaceRoot}/dist/packages/ag-charts-angular.tgz"],
      "options": {
        "cwd": "packages/ag-charts-angular",
        "commands": [
          {
            "command": "mkdir -p ../../dist/packages"
          },
          {
            "command": "yarn pack -f ../../dist/packages/ag-charts-angular.tgz"
          }
        ]
      }
    },
    "pack:extract": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack"],
      "outputs": ["{workspaceRoot}/dist/packages/contents/ag-charts-angular/"],
      "options": {
        "commands": [
          {
            "command": "mkdir -p dist/packages/contents/ag-charts-angular"
          },
          {
            "command": "tar -xzf dist/packages/ag-charts-angular.tgz -C dist/packages/contents/ag-charts-angular/"
          }
        ]
      }
    },
    "pack:verify": {
      "executor": "nx:run-commands",
      "dependsOn": ["pack:extract"],
      "options": {
        "commands": [
          {
            "command": "node tools/sanity-check-package.js dist/packages/contents/ag-charts-angular/package"
          }
        ]
      }
    }
  },
  "tags": []
}
