{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "plugins": ["./plugins/ag-charts-task-autogen"],
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "defaultExcludes": [
      "buildOutputExcludes",
      "!{projectRoot}/.eslintrc.*",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/**/test/**",
      "!{projectRoot}/**/__image_snapshots__/**",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s"
    ],
    "buildOutputExcludes": ["!{projectRoot}/dist/**"],
    "production": ["default", "defaultExcludes"],
    "tsDeclarations": [
      {
        "dependentTasksOutputFiles": "**/*.d.ts",
        "transitive": false
      }
    ],
    "jsOutputs": [
      {
        "dependentTasksOutputFiles": "**/*.js",
        "transitive": false
      }
    ],
    "tsDefaults": [
      "{projectRoot}/package.json",
      "{projectRoot}/src/**/*",
      "{projectRoot}/tsconfig.*.json",
      "tsDeclarations",
      "sharedGlobals",
      "defaultExcludes",
      {
        "externalDependencies": ["npm:typescript", "npm:esbuild"]
      }
    ],
    "sharedGlobals": ["{workspaceRoot}/esbuild.config*.cjs", "{workspaceRoot}/tsconfig.*.json"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "build:types": {
      "dependsOn": ["^build:types"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "build:locale": {
      "dependsOn": [],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "build:package": {
      "dependsOn": ["^build:package"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "build:umd": {
      "dependsOn": ["build:package", "^build:package"],
      "inputs": ["^jsOutputs"],
      "cache": true
    },
    "lint": {
      "inputs": ["default", "buildOutputExcludes", "{workspaceRoot}/.eslintrc.*", "{workspaceRoot}/.eslintignore"],
      "outputs": [],
      "cache": true
    },
    "lint:depcruise": {
      "inputs": ["default", "buildOutputExcludes", "{projectRoot}/.dependency-cruise.js"],
      "outputs": [],
      "cache": true
    },
    "lint:circular": {
      "inputs": ["default", "buildOutputExcludes"],
      "outputs": [],
      "cache": true
    },
    "test": {
      "inputs": ["default", "buildOutputExcludes", "^production", "{workspaceRoot}/jest.preset.js"],
      "outputs": [],
      "cache": true
    },
    "e2e": {
      "inputs": ["default", "buildOutputExcludes", "^production"],
      "outputs": [],
      "cache": true
    },
    "pack": {
      "inputs": [
        "{projectRoot}/dist/**",
        "{projectRoot}/.npmignore",
        "{projectRoot}/README.md",
        "{projectRoot}/LICENSE*",
        "{projectRoot}/package.json"
      ],
      "cache": true
    },
    "@nx/jest:jest": {
      "inputs": ["default", "buildOutputExcludes", "^production", "{workspaceRoot}/jest.preset.js"],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "buildOutputExcludes",
        "{workspaceRoot}/.eslintrc.*",
        "{projectRoot}/.eslintrc.*",
        "{workspaceRoot}/tools/eslint-rules/**/*"
      ],
      "outputs": []
    },
    "@nx/vite:test": {
      "cache": true,
      "inputs": ["default", "buildOutputExcludes", "^production"]
    },
    "nx:noop": {
      "cache": true,
      "inputs": [],
      "outputs": []
    },
    "nx:run-script": {
      "cache": true,
      "inputs": ["production"],
      "outputs": []
    },
    "nx:run-commands": {
      "cache": true,
      "inputs": ["production"],
      "outputs": []
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "libraries"
  },
  "nxCloudAccessToken": "ZDJhN2YwMWMtYWZiMS00MzY2LWFiZDgtZTc0ZTg4MTk4ZDE2fHJlYWQ=",
  "defaultProject": "all",
  "defaultBase": "latest"
}
