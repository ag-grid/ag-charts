{
  "name": "ag-charts-generate-chart-thumbnail",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "plugins/ag-charts-generate-chart-thumbnail/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["ag-charts-community:build:package", "ag-charts-enterprise:build:package"],
      "options": {
        "outputPath": "plugins/ag-charts-generate-chart-thumbnail/dist",
        "main": "plugins/ag-charts-generate-chart-thumbnail/src/index.ts",
        "tsConfig": "plugins/ag-charts-generate-chart-thumbnail/tsconfig.lib.json",
        "assets": [
          "plugins/ag-charts-generate-chart-thumbnail/*.md",
          {
            "input": "./plugins/ag-charts-generate-chart-thumbnail/src",
            "glob": "**/!(*.ts|*.snap)",
            "output": "./src"
          },
          {
            "input": "./plugins/ag-charts-generate-chart-thumbnail/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./plugins/ag-charts-generate-chart-thumbnail",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./plugins/ag-charts-generate-chart-thumbnail",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "plugins/ag-charts-generate-chart-thumbnail/jest.config.ts"
      },
      "configurations": {
        "update": {
          "updateSnapshot": true
        }
      }
    }
  },
  "tags": []
}
